{
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "eslint.format.enable": true,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  "editor.formatOnSave": true,
  "files.exclude": {
    "**/.db": true,
    "**/.git": true,
    "**/.expo": true,
    "**/.next": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/build": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/tsconfig.tsbuildinfo": true
  }
}
